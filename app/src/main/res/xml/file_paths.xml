<?xml version="1.0" encoding="utf-8"?>
<paths xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Đường dẫn cho external files directory (Pictures) -->
    <external-files-path
        name="pictures"
        path="Pictures/" />

    <!-- Đường dẫn cho external cache directory -->
    <external-cache-path
        name="cache"
        path="." />

    <!-- Đường dẫn cho internal files directory -->
    <files-path
        name="files"
        path="." />
</paths>

